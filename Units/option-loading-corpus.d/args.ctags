--langdef=AAA
	--regex-AAA=/def ([A-Z])/\1/d,definition/
	--map-AAA=+.qqq
	--corpus-AAA=.qqq:Units/option-loading-corpus.d/AAA.qqq

--langdef=BBB
	--regex-BBB=/set ([A-Z])/\1/p,parameter/
	--map-BBB=+.qqq
	--corpus-BBB=.qqq:Units/option-loading-corpus.d/BBB.qqq

--langdef=CCC
	--regex-CCC=/var ([A-Z])/\1/v,variable/
	--map-CCC=+.qqq
	--corpus-CCC=.qqq:Units/option-loading-corpus.d/CCC.qqq

--fields=+l
